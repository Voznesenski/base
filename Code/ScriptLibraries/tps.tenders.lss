'++LotusScript Development Environment:2:5:(Options):0:74
Option Public
Option Declare

Use "tps.tenders.approval"
Use "tps.tenders.offers"
Use "tps.tenders.tors"

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Public Class TPS_TendersService As FBase_Abstract_Service

'++LotusScript Development Environment:2:5:(Declarations):0:10


Private Const DATABASE_KEY = "tps.tenders.info"
'======================================================================================================================================
'	CLASS TPS_PaytermsService
'======================================================================================================================================
Public Class TPS_TendersService As FBase_Abstract_Service
	Private m_approval As Variant
	Private m_offers As Variant
	Private m_tors As Variant
	Private m_db As NotesDatabase
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	'	PUBLIC:
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Sub New(in_ps As Variant)
	End Sub
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Function getObject( in_link As Variant ) As Variant
		On Error GoTo error_point
		
		Set getObject = Nothing
		If ( in_link.domain Like "tps.tenders.tors.*" ) Then
			Set getObject = Me.tors.getObject( in_link )
		Else
			Error 1001, "The package for object '" & in_link.domain & "' from parameter 'in_link.domain' is not registered"
		End If
		Exit Function
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Function
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get approval As Variant
		On Error GoTo error_point
		
		If IsEmpty(m_approval)Then
			Set m_approval = New TPS_TendersApprovalService( Me.Me_Variant )
		End If
		Set approval = m_approval
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get offers As Variant
		On Error GoTo error_point
		
		If IsEmpty(m_offers)Then
			Set m_offers = New TPS_TendersOffersService( Me.Me_Variant )
		End If
		Set offers = m_offers
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get tors As Variant
		On Error GoTo error_point
		
		If IsEmpty(m_tors)Then
			Set m_tors = New TPS_TendersTORsService( Me.Me_Variant )
		End If
		Set tors = m_tors
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get db As NotesDatabase
		On Error GoTo error_point
		Call Me.log.enter( TypeName( Me ) & "." & GetThreadInfo(1))
		
		If ( m_db Is Nothing )Then
			Set m_db = Me.sm.base.databases.get( DATABASE_KEY )
		End If
		Set db = m_db
		
		Call Me.log.traceObject( "result", db )	
		Call Me.log.exit( TypeName( Me ) & "." & GetThreadInfo(1))
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	
	
End Class