Option Public
Option Declare

Use "tps.it.accessApproval"


'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Public Class TPS_ItService As Fbase_abstract_service

'++LotusScript Development Environment:2:5:(Declarations):0:10
Public Class TPS_ItService As Fbase_abstract_service
	Private m_accessApproval As Variant
	'constructor
	Public Sub New(in_ps As Variant)
		'nothing
	End Sub
	
	
	Property Get accessApproval As Variant
		On Error GoTo error_point
		
		If IsEmpty( m_accessApproval ) Then
			Set m_accessApproval = New TPS_It_AccessApprovalService( Me.Me_variant )
		End If
		Set accessApproval = m_accessApproval
		
		Exit Property
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	
	
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------	
	Public Function getObject( in_link As Variant ) As Variant
		On Error GoTo error_point
		
		Set getObject = Nothing
		If ( in_link.domain Like "tps.it.accessApproval.*" ) Then
			Set getObject = me.accessApproval.getObject( in_link )
			
		Else
			Error 1001, "The package for object '" & in_link.domain & "' from parameter 'in_link.domain' is not registered"
			
		End If


		Exit Function
		
error_point:
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$		
	End Function
	
	
End Class
