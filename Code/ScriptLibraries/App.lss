'++LotusScript Development Environment:2:5:(Options):0:74
%REM
	Library app
%END REM

Option Public
Option Declare

Use "fbyte"
Use "tps"


'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Class ApplicationService As FBase_Abstract_Service
Declare Function getApplicationService( in_ps As Variant ) As Variant

'++LotusScript Development Environment:2:5:(Declarations):0:10
'======================================================================================================================================
'	CLASS ApplicationService
'======================================================================================================================================
Class ApplicationService As FBase_Abstract_Service
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	'	PUBLIC:
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Sub New ( in_ps As Variant )
	End Sub
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------	
	Property Get tps As Variant
		On Error Goto error_point
		Call Me.log.enter( Typename( Me ) & "." & Getthreadinfo(1))
		
		Set tps = Me.sm.get( "tps" )
		
		Call Me.log.exit( "</" & Typename( Me ) & "." & Getthreadinfo(1))
		Exit Property
		
error_point:
		On Error Goto 0
		Error Err, Typename( Me ) & "." & Getthreadinfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	
	
End Class
'++LotusScript Development Environment:2:1:getApplicationService:1:8
Function getApplicationService( in_ps As Variant ) As Variant
	On Error Goto error_point
	
	Dim pckgTPS As Variant
	Set pckgTPS = getPackageObject( "tps", "getTPSService" )
	Call in_ps.set( pckgTPS )
	
	Set getApplicationService = New ApplicationService( in_ps )
	Exit Function
	
error_point:
	On Error Goto 0
	Error Err, Getthreadinfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$	
End Function












