'++LotusScript Development Environment:2:5:(Options):0:74
Option Public
Option Declare

Use "tps.finance.budgets"
Use "tps.finance.estimates"

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Public Class TPS_FinanceService As FBase_Abstract_Service

'++LotusScript Development Environment:2:5:(Declarations):0:10
'======================================================================================================================================
'	CLASS TPS_FinanceService
'======================================================================================================================================
Public Class TPS_FinanceService As FBase_Abstract_Service
	Private m_budgets As Variant
	Private m_estimates As Variant
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	'	PUBLIC:
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Sub New(in_ps As Variant)
	End Sub
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Function getObject( in_link As Variant ) As Variant
		On Error GoTo error_point
		
		Set getObject = Nothing
		If ( in_link.domain Like "tps.finance.budgets.*" ) Then
			Set getObject = Me.budgets.getObject( in_link )
		Else
			Error 1001, "The package for object '" & in_link.domain & "' from parameter 'in_link.domain' is not registered"
		End If
		Exit Function
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Function
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get budgets As Variant
		On Error GoTo error_point
		
		If IsEmpty( m_budgets )Then
			Set m_budgets = New TPS_Finance_BudgetsService( Me.Me_Variant )
		End If
		Set budgets = m_budgets
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Property Get estimates As Variant
		On Error GoTo error_point
		
		If IsEmpty( m_budgets )Then
			Set m_estimates = New TPS_Estimate_Service ( Me.Me_Variant )
		End If
		Set estimates = m_estimates
		Exit Property
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$
	End Property
	
	
End Class