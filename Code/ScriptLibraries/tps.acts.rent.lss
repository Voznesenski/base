	'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	Function getActsByContractId( in_key As String ) As mixcollection
		On Error GoTo error_point
		Call Me.log.enter( TypeName( Me ) & "." & GetThreadInfo(1))
		Call Me.log.parameter( "in_key", in_key )
		
		Dim mix As New mixcollection
		Set getActsByContractId = mix
		
		Dim view As NotesView
		Set view = me.db.Getview("(app.acts.byContractId)")
		
		Dim col As NotesDocumentCollection
		Dim doc As NotesDocument
		
		Set col = view.Getalldocumentsbykey(in_key, True)
		If(col.count = 0)Then Exit Function
		
		Set doc = col.Getfirstdocument()
		While Not(doc Is Nothing)
			Dim act As Variant
			Set act = me.getDocObject(doc)
			Call mix.Append(act)
			
			Set doc = col.Getnextdocument(doc)
		Wend
		
		
		Call Me.log.traceObject( "result", getActsByContractId )
		Call Me.log.exit( TypeName( Me ) & "." & GetThreadInfo(1))
		Exit Function
		
error_point:
		On Error GoTo 0
		Error Err, TypeName( Me ) & "." & GetThreadInfo(1) & " ( nm: " & Err & ", ln: " & Erl & " ) >> " & Error$ 
	End Function
	
